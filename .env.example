AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Samsara API Configuration
SAMSARA_API_TOKEN=
SAMSARA_API_URL=https://api.samsara.com

# Samsara API Endpoints (optional - defaults provided)
# SAMSARA_VEHICLES_ENDPOINT=/fleet/vehicles/stats/feed
# SAMSARA_TRAILERS_ENDPOINT=/fleet/trailers/stats/feed
# SAMSARA_DRIVERS_ENDPOINT=/fleet/drivers
# SAMSARA_ADDRESSES_ENDPOINT=/addresses

# Samsara Sync Configuration
SAMSARA_SYNC_TIMEOUT=30
SAMSARA_SYNC_RETRY_TIMES=3
SAMSARA_SYNC_RETRY_DELAY=1000
SAMSARA_SYNC_PAGE_LIMIT=100

# Sync Schedule Configuration
SAMSARA_SYNC_FREQUENCY="*/5"
SAMSARA_SYNC_VEHICLES_ENABLED=true
SAMSARA_SYNC_TRAILERS_ENABLED=true
SAMSARA_SYNC_DRIVERS_ENABLED=false

# Operating Hours (24-hour format)
SAMSARA_SYNC_START_HOUR=6
SAMSARA_SYNC_END_HOUR=22
SAMSARA_SYNC_WEEKDAYS_ONLY=false

# Default Tag IDs (comma-separated, optional)
# SAMSARA_DEFAULT_TAG_IDS=123,456,789

# Logging Configuration
SAMSARA_LOG_CHANNEL=daily
SAMSARA_LOG_LEVEL=info
SAMSARA_LOG_API_REQUESTS=false
SAMSARA_LOG_SYNC_DETAILS=true
